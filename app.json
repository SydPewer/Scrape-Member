    "SESSION_STRING": {
            "description": "Session string, Check the readme if you don't know how to get this.",
            "required": true
    },
    "BOT_TOKEN": {
	    "description": "Get this from @Botfather.",
	    "required": true
    }},
    "formation": {
      "worker": {
        "quantity": 1,
        "size": "free"
      }
  }
